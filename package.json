{
  "name": "@nextcloud/files",
  "version": "3.5.1",
  "description": "Nextcloud files utils",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Christoph Wurst <christoph@winzerhof-wurst.at>",
  "contributors": [
    "John Molakvo√¶ <skjnldsv@protonmail.com>",
    "Ferdinand Thiessen <opensource@fthiessen.de>"
  ],
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "nextcloud",
    "files",
    "library"
  ],
  "scripts": {
    "build": "vite --mode production build",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "dev": "vite --mode development build",
    "watch": "vite --mode development build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-files.git"
  },
  "bugs": {
    "url": "https://github.com/nextcloud-libraries/nextcloud-files/issues"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-files",
  "devDependencies": {
    "@codecov/vite-plugin": "^0.0.1-beta.10",
    "@nextcloud/eslint-config": "^8.4.1",
    "@nextcloud/typings": "^1.8.0",
    "@nextcloud/vite-config": "^2.0.2",
    "@types/node": "^20.14.5",
    "@vitest/coverage-istanbul": "^1.6.0",
    "fast-xml-parser": "^4.4.0",
    "jsdom": "^24.1.0",
    "tslib": "^2.6.3",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@nextcloud/auth": "^2.3.0",
    "@nextcloud/l10n": "^3.1.0",
    "@nextcloud/logger": "^3.0.2",
    "@nextcloud/paths": "^2.1.0",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/sharing": "^0.2.1",
    "cancelable-promise": "^4.3.1",
    "is-svg": "^5.0.1",
    "typescript-event-target": "^1.1.1",
    "webdav": "^5.6.0"
  }
}
